package com.zeib.learnspringframework.game;

public class GameRunner {
	public MarioGame game;
//	public MarioGame game;
	public GameRunner(MarioGame game) {
		this.game=game;
	}
	  public void run() {
		System.out.println("Running game"+game);
		game.down();
		game.up();
		game.left();
		game.right();
	}
}
