import java.util.ArrayList;
import java.util.List;
import java.util.*;
public class ListDemo {

	public static void main(String[] args) {
		List l=new ArrayList();
		l.add(0,10);
		l.add(1,20);
		l.add(2,20);
		l.add(3,30);
		l.add(4,40);
//		System.out.println(l);
		
		Iterator itr=l.iterator();
		while(itr.hasNext()) {
			System.out.println(itr);
		}
	}

}
